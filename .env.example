# 数据库密码 - 请设置一个强密码
POSTGRES_PASSWORD=your_secure_postgres_password_here

# 域名配置 - 替换为你的实际域名
DOMAIN=litellm.yourdomain.com

# DeepSeek API Key - 从 https://platform.deepseek.com 获取
DEEPSEEK_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# LiteLLM 主密钥 - 用于 API 认证
LITELLM_MASTER_KEY=sk-1234567890abcdef

# LiteLLM 盐值密钥 - 用于加密
LITELLM_SALT_KEY=your_random_salt_key_here

# 可选：Web UI 登录凭据
UI_USERNAME=admin
UI_PASSWORD=your_secure_ui_password

# 可选：其他配置
# LITELLM_LOG=INFO
# LITELLM_REQUEST_TIMEOUT=600
